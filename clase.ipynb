      "import numpy as np\n",
      "import matplotlib.pylab as plt\n",
      "def potencial(x):\n",
      "    return ((10-x)**0.5)*np.tan((10-x)**0.5)-(x)*0.5\n",
      "n_points=10000\n",
      "X= np.linspace(0,np.pi,n_points)\n",
      "f=potencial(X)\n",
      "plt.plot(X,f) "
